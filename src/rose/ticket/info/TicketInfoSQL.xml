<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC ".//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
 "http://ibatis.apache.org/dtd/sql-map-2.dtd">
 <sqlMap namespace="ticket">
 	<typeAlias alias="dto" type="rose.ticket.info.TicketInfoDTO"/>
 	
 	<resultMap class="java.util.list" id="resultList">
 		<result property="ticket_id" column="ticket_id"/>
 		<result property="no" column="no"/>
 		<result property="seat_id" column="seat_id"/>
 		<result property="screen_id" column="screen_id"/>
 		<result property="screen_name" column="ticket_id"/>
 		<result property="screen_type" column="ticket_id"/>
 		<result property="screen_typename" column="ticket_id"/>
 		<result property="theater_id" column="ticket_id"/>
 		<result property="theater_name" column="ticket_id"/>
 		<result property="line_no" column="ticket_id"/>
 		<result property="seat_no" column="ticket_id"/>
 		<result property="pos_x" column="ticket_id"/>
 		<result property="pos_y" column="ticket_id"/>
 		<result property="seat_type" column="ticket_id"/>
 		<result property="seat_typename" column="ticket_id"/>
 		<result property="price" column="ticket_id"/>
 		<result property="buy_date" column="ticket_id"/>
 		<result property="member_id" column="ticket_id"/>
 		<result property="mcard_id" column="ticket_id"/>
 		<result property="buy_type" column="ticket_id"/>
 		<result property="movie_id" column="ticket_id"/>
 		<result property="movie_name" column="ticket_id"/>
 		<result property="date_info" column="ticket_id"/>
 		<result property="start_time" column="start_time"/>
 		<result property="end_time" column="end_time"/>
 		<result property="coupon_id" column="coupon_id"/>
 	</resultMap>
 	
 	<insert id="insertTest" parameterClass="dto">
 		INSERT INTO TICKET_INFO (TICKET_ID, NO, SEAT_ID, SCREEN_ID) VALUES(TICKET_ID_SEQ.NEXTVAL, #no#, #seat_id#, #screen_id#)
 	</insert>
 	
 	<select id="selectAllTicket" resultClass="resultList">
 		SELECT * FROM TICKET_INFO
 	</select>

 </sqlMap>